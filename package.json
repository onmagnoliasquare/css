{
  "name": "oms-tachyons",
  "version": "1.0.0",
  "description": "OMS Tachyons Fork",
  "author": "n30w",
  "style": "css/tachyons.min.css",
  "main": "css/tachyons.css",
  "files": ["css", "src"],
  "repository": "tachyons-css/tachyons",
  "keywords": [
    "css",
    "oocss",
    "postcss",
    "functional css",
    "design",
    "responsive",
    "performance"
  ],
  "license": "MIT",
  "devDependencies": {
    "copy-files": "^0.1.0",
    "immutable-css-cli": "^1.1.1",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.3",
    "postcss-rtl": "^2.0.0",
    "tachyons-cli": "^1.3.3",
    "tachyons-modules": "^1.1.10",
    "vite": "^5.2.7",
    "watch": "^1.0.2"
  },
  "contributors": [
    {
      "name": "adam morse",
      "email": "hi@mrmrs.cc"
    },
    {
      "name": "john otander",
      "url": "http://johnotander.com"
    },
    {
      "name": "neo alabastro",
      "email": "neo.alabastro@nyu.edu"
    }
  ],
  "scripts": {
    "dev": "yarn vite",
    "start": "yarn build:watch",
    "mutations": "yarn immutable-css src/tachyons.css --strict",
    "build": "./build.sh",
    "build:watch": "yarn watch 'yarn tachyons src/tachyons.css --preserve-variables > css/tachyons.css && yarn tachyons src/tachyons.css --minify --preserve-variables > css/tachyons.min.css' src/"
  },
  "packageManager": "yarn@4.1.1+sha256.f3cc0eda8e5560e529c7147565b30faa43b4e472d90e8634d7134a37c7f59781"
